# Input: numbers separated by a space
numbers = input().split()

# Convert input to floats
numbers = [float(num) for num in numbers]

# Use a set to store unique numbers
unique_numbers = set(numbers)

# Print occurrences using the set and the count method
for num in unique_numbers:
    count = numbers.count(num)  # Count occurrences of each unique number
    print(f"{num:.1f} - {count} times")
        
