using System;
using System.Collections.Generic;

namespace _8._Balanced_Parentheses
{
    class Program
    {
        static void Main(string[] args)
        {
            var parentheses = new Stack<char>();
            string inputSequence = Console.ReadLine();
            bool isBalanced = true;

            for (int i = 0; i < inputSequence.Length; i++)
            {
                if (inputSequence[i] == '{' || inputSequence[i] == '[' || inputSequence[i] == '(')
                {
                    parentheses.Push(inputSequence[i]);
                    continue;
                }
                if (parentheses.Count == 0)
                {
                    isBalanced = false;
                    break;
                }
                if (parentheses.Peek() == '(' && inputSequence[i] == ')')
                {
                    parentheses.Pop();
                    continue;
                }
                if (parentheses.Peek() == '[' && inputSequence[i] == ']')
                {
                    parentheses.Pop();
                    continue;
                }
                if (parentheses.Peek() == '{' && inputSequence[i] == '}')
                {
                    parentheses.Pop();
                    continue;
                }
            }
            Console.WriteLine(parentheses.Count == 0 && isBalanced == true ? "YES" : "NO");
        }
    }
}
