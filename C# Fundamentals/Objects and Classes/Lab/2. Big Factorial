using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Numerics;

namespace _2.Big_Factorial_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());
            BigInteger digit = 1;


            for (int i = 2; i <= N; i++)
            {
                digit = digit * i;
            }
            Console.WriteLine(digit);
        }
    }
}
